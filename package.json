{
    "name": "my-plugin",
    "description": "## Installation",
    "version": "1.6.83",
    "engines": {
        "sketch": ">=49.0"
    },
    "skpm": {
        "name": "wooh",
        "manifest": "src/manifest.json",
        "main": "wooh.sketchplugin",
        "assets": [
            "assets/**/*"
        ],
        "sketch-assets-file": "sketch-assets/icons.sketch"
    },
    "scripts": {
        "build": "skpm-build",
        "watch": "skpm-build --watch",
        "start": "skpm-build --watch --run",
        "postinstall": "npm run build && skpm-link"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@skpm/extract-loader": "^2.0.2",
        "@skpm/builder": "^0.7.4",
        "css-loader": "^3.2.0",
        "html-loader": "^0.5.5"
    },
    "resources": [
        "resources/**/*.js"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Woohuang/SketchWooh"
    },
    "dependencies": {
        "@babel/polyfill": "^7.11.5",
        "@skpm/fs": "^0.2.6",
        "@types/node": "^14.0.13",
        "openurl": "^1.1.1",
        "sketch-module-web-view": "^3.4.3",
        "sketch-polyfill-fetch": "^0.5.2",
        "skpm": "^1.3.2"
    },
    "bugs": {
        "url": "https://github.com/Woohuang/SketchWooh/issues"
    },
    "homepage": "https://github.com/Woohuang/SketchWooh#readme",
    "main": "index.js",
    "keywords": [],
    "author": "",
    "license": "ISC"
}
